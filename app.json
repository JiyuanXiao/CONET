{
  "expo": {
    "name": "CONET",
    "slug": "CONET",
    "version": "1.1.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon_v2.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-transparent.png",
      "resizeMode": "cover",
      "backgroundColor": "#fcc404"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ianxiao.CONET",
      "buildNumber": "2"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon_v2.png",
        "backgroundColor": "#fcc404"
      },
      "package": "com.ianxiao.CONET",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "允许访问你的相册"
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "允许访问你的相册",
          "savePhotosPermission": "允许保存图片到你的相册",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notificationicon.png",
          "color": "#fcc404",
          "defaultChannel": "default",
          "sounds": [
            "./assets/sounds/correctanswer.wav",
            "./assets/sounds/longpop.wav"
          ]
        }
      ],
      [
        "expo-av",
        {
          "microphonePermission": "允许访问你的麦克风"
        }
      ],
      [
        "expo-asset",
        {
          "assets": []
        }
      ],
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "42c68c61-5942-4270-83d7-83fd8e8b3b7b"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/42c68c61-5942-4270-83d7-83fd8e8b3b7b"
    }
  }
}
